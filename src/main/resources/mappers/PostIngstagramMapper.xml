<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace = "com.ashspell.ingstagram.post.dao.IngstagramDAO">
 <insert id = "postUser" parameterType = "map">
 	 INSERT INTO `ingstagram`(
 	 `id`
 	 , `loginId`
 	 , `imagePath`
 	 , `title`
 	 , `content`
 	 , `createdAt`
 	 , `updatedAt`
 	 
 	 ) value (
 	  #{id}
 	  , #{loginId}
 	  , #{imagePath}
 	  , #{title}
 	  , #{content}
 	  , now();
 	  , now(); 
 	 )
 	 </insert>
 	
 	<select id = "selectPostUser" parameterType = "map">
 		SELECT
 			`id` 
 			, `imagePath`
 			, `title`
 			, `content`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`ingstagram`
 	</select>
 	

 	
 	
 	
 	
 </mapper>